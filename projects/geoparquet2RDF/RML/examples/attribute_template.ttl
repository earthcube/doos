@base <http://example.com/ns#> .

<https://example.org/AttributeMapping/source> a rml:LogicalSource ;
    rml:source
        [
            a sd:DatasetSpecification ;
            sd:name "variable1" ;
            sd:hasDataTransformation
                [
                    sd:hasSoftwareRequirements "pandas>=1.1.0" ;
                    sd:hasSourceCode
                        [
                            sd:programmingLanguage "Python3.9" ;
                        ] ;
                ] ;
        ] ;
    rml:referenceFormulation rml:DataFrame .

<https://example.org/AttributeMapping> a rml:TriplesMap ;

    schema:name "OHDSI Attribute mapping";

    rml:logicalSource <https://example.org/AttributeMapping/source> ;


    rml:subjectMap
        [
            a rml:SubjectMap ;
            rml:template "http://ohdsi_gis.org/id/{hashid}" ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant rdf:type ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:constant schema:Dataset ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:variableMeasured ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:template "https://ohdsi_gis.org/.well-known/genid/{hashid}/{var_name}" ;
                ] ;
        ] .


<https://example.org/DatasetMapping/Variable> a rml:TriplesMap ;

    rml:logicalSource
        [
            a rml:LogicalSource ;
            rml:source
                [
                    a sd:DatasetSpecification ;
                    sd:name "variable1" ;
                    sd:hasDataTransformation
                        [
                            sd:hasSoftwareRequirements "pandas>=1.1.0" ;
                            sd:hasSourceCode
                                [
                                    sd:programmingLanguage "Python3.9" ;
                                ] ;
                        ] ;
                ] ;
            rml:referenceFormulation rml:DataFrame ;
        ] ;


    rml:subjectMap
        [
            a rml:SubjectMap ;
            rml:template "https://ohdsi_gis.org/.well-known/genid/{hashid}/{var_name}" ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant rdf:type ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:constant schema:PropertyValue ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:measurementMethod ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_source" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:description ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_desc" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:name ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_name" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:additionalType ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_dtype" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:unitsText ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_units_text" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:unitsConceptId ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_units_concept_id" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:startDate ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_start_date" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:endDate ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_end_date" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:conceptId ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "var_concept_id" ;
                ] ;
        ] ;

    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
