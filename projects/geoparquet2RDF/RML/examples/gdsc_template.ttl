<thing_0> a rml:TriplesMap ;

    rml:logicalSource
        [
            a rml:LogicalSource ;
            rml:source
                [
                    a sd:DatasetSpecification ;
                    sd:name "variable1" ;
                    sd:hasDataTransformation
                        [
                            sd:hasSoftwareRequirements "pandas>=1.1.0" ;
                            sd:hasSourceCode
                                [
                                    sd:programmingLanguage "Python3.9" ;
                                ] ;
                        ] ;
                ] ;
            rml:referenceFormulation rml:DataFrame ;
        ] ;


    rml:subjectMap
        [
            a rml:SubjectMap ;
            rml:template "http://ohdsi_gis.org/id/{hashid}" ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant rdf:type ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:constant schema:Dataset ;
                ] ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:name ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "dct_title" ;
                ] ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:license ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "dct_license" ;
                ] ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:keywords ;
                ] ;
            rml:objectMap
                [
#                    rml:functionExecution <#ExKeywords> ;
                     a rml:ObjectMap ;
                    rml:reference "dcat_keyword" ;
                ] ;

        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:description ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:reference "dct_description" ;
                ] ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:spatialCoverage ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:template "https://ohdsi_gis.org/.well-known/genid/{hashid}" ;
                ] ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant schema:publisher ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:template "{dct_publisher} {dct_rights}" ;
                    rml:datatype rdfs:Literal
                ] ;
        ] .


