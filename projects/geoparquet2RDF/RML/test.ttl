<thing_0> a rml:TriplesMap ;

    rml:logicalSource
        [
            a rml:LogicalSource ;
            rml:source
                [
                    a sd:DatasetSpecification ;
                    sd:name "variable1" ;
                    sd:hasDataTransformation
                        [
                            sd:hasSoftwareRequirements "pandas>=1.1.0" ;
                            sd:hasSourceCode
                                [
                                    sd:programmingLanguage "Python3.9" ;
                                ] ;
                        ] ;
                ] ;
            rml:referenceFormulation rml:DataFrame ;
        ] ;


    rml:subjectMap
        [
            a rml:SubjectMap ;
            rml:template "http://ohdsi_gis.org/id/{title}" ;
        ] ;


    rml:predicateObjectMap
        [
            rml:predicateMap
                [
                    a rml:PredicateMap ;
                    rml:constant rdf:type ;
                ] ;
            rml:objectMap
                [
                    a rml:ObjectMap ;
                    rml:constant schema:Dataset ;
                ] ;
        ] .
